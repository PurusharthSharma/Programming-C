/*          A Program to get the Value and Address of the Elements of the Array         */ 


#include<stdio.h>
#include<conio.h>

void main(){
  int arr[5]={1,2,3,4,6};
  printf("%u",arr);
  int *ptr,i=0;
  ptr=arr;
  while(i<=4)  {
    printf("\nAddress : %u,\nElement : %d\n",ptr,*ptr);
    ptr++;
    i++;
  }
  
  getch();
}
