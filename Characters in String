/*        A Program to find the total number of Characters in the string        */


#include<stdio.h>
#include<conio.h>

void main(){
  char str[20];
  int i,count=0;
  printf("Enter a string : ");
  gets(str);
  for(i=0;str[i]!='\0';i++)  {
    if(str[i]!=' ')
    count++;
  }
  printf("\nNumber of character : %d",count);
  getch();
}
