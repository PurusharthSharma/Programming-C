/*          A Program to Display Matrix using Pointers              */



#include<stdio.h>
#include<conio.h>


void main(){
    int a[3][3]={
      {1,6,3},
      {4,5,2},
      {1,0,5},
    };
    
    int i,j;
    printf("The matrix is : \n");
    for(i=0;i<3;i++){
      for(j=0;j<3;j++){
         printf("%d\t",*(*(a+i)+j));
       }
      printf("\n");
    }
    
    getch();
}
