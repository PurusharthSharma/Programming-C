/*       A Program to find Total Number of Character in the String        */


#include<stdio.h>
#include<conio.h>

void main(){

  char str[20];
  int i,count=0
  
  printf("Enter a string : ");
  gets(str);
  
  for(i=0;str[i]!='\0';i++)
  count++;
  
  printf("\nNumber of character is : %d",count);
  
  getch();
  
}
