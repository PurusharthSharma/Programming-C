/*      A Program to Copy String into an empty String     */


#include<stdio.h>
#include<conio.h>
void main()
{
 char s1[100],s2[100],i;
 clrscr();
 printf("\nEnter the first string  :");
 scanf("%s",&s1);
 for(i=0;s1[i]!='\0';++i)
 {
  s2[i]=s1[i];
 }
 s2[i]='\0';
 printf("\nThe second string is  :%s",s2);

 getch();
}
