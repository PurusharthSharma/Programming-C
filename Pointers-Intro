/*      Introduction to Pointers. A Program to check Wheter the two pointers are at same location or at different Location       */

#include<stdio.h>
#include<conio.h>

void main(){
  int *ptr1,*ptr2;
  int i=4;
  ptr1=&i;
  ptr2=ptr1;
  if(ptr2=ptr1)
    printf("Both have same location");
  else
    printf("Both have diferent location");
  getch();
}
