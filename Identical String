/*        A Program to Check Whether the two Strings are identical or Not          */


#include<stdio.h>
#include<conio.h>
#include<string.h>

void main(){

  char *str1="allahabad";
  char *str2="varanasi";
  
  int i,f=0,l1,l2;
  
  l1=strlen(str1);
  l2=strlen(str2);
  
  if(l1!=l2){
    printf("String are not identical");
  }
  else{
    for(i=0;str1 [i]!='\0';i++){
      if(str1[i]!=str2[i]){
        f=1;
        printf("Strings are not identical");
      }
    }
    if(f==0)
      printf("Strings are identical");
  }
  
getch();

}
